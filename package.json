{
	"name": "bootstrap-sass",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"build:css": "node-sass --include-path=node_modules/bootstrap/scss --output-style compressed bootstrap/bootstrap.scss static/css/bootstrap.min.css",
