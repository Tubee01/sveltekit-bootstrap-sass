		"@sveltejs/kit": "^1.5.0",
		"@types/pg": "^8.6.6",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"bcrypt": "^5.1.0",
		"bootstrap": "^5.2.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-sass": "^8.0.0",
		"passport": "^0.6.0",
		"passport-local": "^1.0.0",
		"pg": "^8.10.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
